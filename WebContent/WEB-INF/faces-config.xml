	<!-- Database Login Navigation Rule -->
	<navigation-rule>
		<display-name>index</display-name>
		<from-view-id>/index.jsp</from-view-id>
		<navigation-case>
			<from-outcome>SUCCESS</from-outcome>
			<to-view-id>/main.jsp</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>FAIL</from-outcome>
			<to-view-id>/index.jsp</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>transaction log</display-name>
		<from-view-id>/transactionLog.jsp</from-view-id>
		<navigation-case>
			<from-outcome>LOGOUT</from-outcome>
			<to-view-id>/logout.jsp</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>dataStats</display-name>
		<from-view-id>/dataStats.jsp</from-view-id>
		<navigation-case>
			<from-outcome>LOGOUT</from-outcome>
			<to-view-id>/logout.jsp</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>dbImport</display-name>
		<from-view-id>/dbImport.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>LOGOUT</from-outcome>
			<to-view-id>/logout.jsp</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>main</display-name>
		<from-view-id>/main.jsp</from-view-id>
		<navigation-case>
			<from-outcome>LOGOUT</from-outcome>
			<to-view-id>/logout.jsp</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>dbReports</display-name>
		<from-view-id>/dbReports.jsp</from-view-id>
		<navigation-case>
			<from-outcome>LOGOUT</from-outcome>
			<to-view-id>/logout.jsp</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>dbVisual</display-name>
		<from-view-id>/dbVisual.jsp</from-view-id>
		<navigation-case>
			<from-outcome>LOGOUT</from-outcome>
			<to-view-id>/logout.jsp</to-view-id>
		</navigation-case>
	</navigation-rule>
		<navigation-rule>
		<display-name>dbCompute</display-name>
		<from-view-id>/dbCompute.jsp</from-view-id>
		<navigation-case>
			<from-outcome>LOGOUT</from-outcome>
			<to-view-id>/logout.jsp</to-view-id>
		</navigation-case>
	</navigation-rule>
</faces-config>